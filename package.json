{
  "name": "MattShop",
  "version": "1.0.0",
  "main": "index.tsx",
  "scripts": {
    "start": "expo start --clear",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "prettier": "prettier --plugin '@trivago/prettier-plugin-sort-imports' --log-level error --write src/**/*.{js,ts,jsx,tsx}",
    "check-typescript": "tsc --noEmit",
    "check-eslint": "eslint './src/**/*{js,ts,jsx,tsx}'",
    "lint": "npm run check-typescript && npm run check-eslint",
    "release": "standard-version",
    "prepare": "husky",
    "test": "jest"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint './src/**/*{js,ts,jsx,tsx}' --fix",
      "prettier --write './src/**/*{js,ts,jsx,tsx}'"
    ]
  },
  "keywords": [
    "react-native",
    "expo",
    "typescript",
    "template"
  ],
  "author": "Vladimir Vovk",
  "license": "MIT",
  "homepage": "https://github.com/vladimir-vovk/expo-ts",
  "bugs": {
    "url": "https://github.com/vladimir-vovk/expo-ts/issues"
  },
  "dependencies": {
    "@expo/metro-runtime": "~4.0.0",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-material/core": "^1.3.7",
    "@react-native-vector-icons/material-icons": "^0.0.1",
    "@react-navigation/bottom-tabs": "^7.2.1",
    "@react-navigation/native": "^7.0.15",
    "@react-navigation/stack": "^7.1.2",
    "@reduxjs/toolkit": "^2.6.0",
    "axios": "^1.8.1",
    "expo": "~52.0.11",
    "expo-status-bar": "~2.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.3",
    "react-native-gesture-handler": "^2.24.0",
    "react-native-reanimated": "^3.17.1",
    "react-native-safe-area-context": "^5.3.0",
    "react-native-screens": "^4.9.1",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "~0.19.13",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.8.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~18.3.12",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~8.0.1",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-testing-library": "^7.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-expo": "^52.0.2",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "standard-version": "^9.5.0",
    "typescript": "^5.3.3",
    "@react-native-community/cli": "latest"
  },
  "private": true
}
